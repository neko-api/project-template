{
    "name": "neko-api/${NAME_CAN}$",
    "type": "library",
    "description": "NekoAPI ${NAME}$ ${PROJECT_TYPE}$",
    "keywords": ["${NAME_CAN}$", "neko-api"],
    "homepage": "https://github.com/neko-api/${NAME_CAN}$",
    "license": "MIT",
    "authors": [
        {
            "name": "${AUTHOR}$",
            "email": "${EMAIL}$"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": { "NekoAPI\\${PROJECT_TYPE}$\\${NAME}$\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NekoAPI\\${PROJECT_TYPE}$\\${NAME}$\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
